  <entry>
    <string>au_name</string>
    <string>"HighWire eLife Drupal Plugin (CLOCKSS), Base URL %s, Volume %s", base_url, volume_name</string>
  </entry>
  <entry>
    <string>au_start_url</string>
    <string>"%sclockss-manifest/elife_%s.html", base_url, volume_name</string>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
      <!-- no gifs from other volumes
      <string>2,"^%scontent/(?!([^/]+/)?%s)[^?]*[.]gif", base_url, volume_name</string>
      <string>1,"^https?://%s[^?]*/highwire/jcore", base_url_host</string>
       -->
      <!-- permission page & all resources on any CDN (cloudfront host) -->
      <string>1,"^https?://[^.]+[.]cloudfront[.]net/(lockss[.]htm|[^?]+[.](bmp|gif|ico|jpe?g|png|tif?f|css|js))$"</string>
      <!-- all images, resources, css, js, etc. -->
      <string>1,"^https?://%s/[^?]*[.](bmp|gif|ico|jpe?g|png|tif?f|(css|js)([?].*)?)$", base_url_host</string>
      <!-- Do not crawl unless from base url -->
      <string>4,"^%s", base_url</string>
      <string>1,"^%s(content/[^/]+/e[^/?]+$|highwire/(filestream|markup)/|elife/download-video/)", base_url</string>
      <string>1,"^%scontent(/elife)?/[^/]+/e[^/?]+([.]full[.]pdf$|/article-(data|info)|/.*/embed/)", base_url</string>
      <string>1,"^%shighwire/citation/[^/]+/ris$", base_url</string>
    </list>
  </entry>
  <entry>
    <string>au_substance_url_pattern</string>
    <list>
      <string>"^%scontent(/elife)?/[^/]+/e[^./]+([.]full([.]pdf)?)?$", base_url</string>
    </list>
  </entry>
