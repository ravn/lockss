  <entry>
    <string>required_daemon_version</string>
    <!-- needs 1.66 for ris filter -->
    <string>1.66.0</string>
  </entry>
  <entry>
    <string>plugin_name</string>
    <string>Palgrave Macmillan Book Plugin (CLOCKSS)</string>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>book_isbn</key>
        <displayName>Book ISBN</displayName>
        <description>International Standard Book Number</description>
        <type>1</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>
  <entry>
    <string>au_name</string>
    <string>"Palgrave Macmillan Book Plugin (CLOCKSS), Base URL %s, ISBN %s", base_url, book_isbn</string>
  </entry>
  <entry>
    <string>au_start_url</string>
    <string>"%spc/doifinder/10.1057/%s", base_url, book_isbn</string>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <!-- examples
    http://www.palgraveconnect.com/pc/socialsciences2013/browse/inside/chapter/9781137268839/9781137268839.0001.html?chapterDoi=9781137268839.0001
    http://www.palgraveconnect.com/pc/browse/citationExport?doi=10.1057/9781137310149&WT.cg_n=eBooks&WT.cg_s=Citation%20Export
    http://www.palgraveconnect.com/pc/doifinder/10.1057/9781137284808.0007
    http://www.palgraveconnect.com/pc/education2013/browse/inside/download/9781137021052.pdf
    -->
    <list>
      <!-- removes gifs for fb, reddit, etc.. -->
      <string>2,"^%sview/images/.*\.gif", base_url</string> 
      <string>1,"^https?://%s/.*\.(bmp|css|gif|ico|jpe?g|js|png|tif?f)$", base_url_host</string>
      <string>4,"^%s", base_url</string>
      <string>1,"^%spc/[^/]+/browse/inside/", base_url</string>
      <string>1,"^%spc/browse/citationExport\?doi=10\.1057/%s[^/]+Export$", base_url, book_isbn</string>
      <string>1,"^%spc/doifinder/10\.1057/%s\.\d+$", base_url, book_isbn</string>
    </list>
  </entry>
  <entry>
    <string>au_substance_url_pattern</string>
    <!-- examples
    http://www.palgraveconnect.com/pc/education2013/browse/inside/download/9781137021052.pdf
    http://www.palgraveconnect.com/pc/socialsciences2013/browse/inside/epub/9781137268839.epub
    -->
    <list>
    <!--  <string>"^%spc/[^/]+/browse/inside(/[^/]+)?/%s\.(html|pdf|epub)$", base_url, book_isbn</string> -->
    <string>"^%spc/[^/]+/browse/inside/[^/]+/%s\.(pdf|epub)$", base_url, book_isbn</string>
    </list>
  </entry>
  <entry>
    <string>au_def_pause_time</string>
    <long>3000</long>
  </entry>
  <entry>
    <string>au_def_new_content_crawl</string>
    <long>1209600000</long>
  </entry>
