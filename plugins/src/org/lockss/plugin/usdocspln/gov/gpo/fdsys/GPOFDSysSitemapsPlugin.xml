      <!-- this very next rate is ignored -->
      <rate>1/3s</rate>
      <cond>
        <!-- pause crawls Monday through Friday 9am through 9pm Eastern (12h) -->
        <entry>
          <org.lockss.daemon.CrawlWindows-Daily>
            <from>9:00</from>
            <to>21:00</to>
            <timeZoneId>US/Eastern</timeZoneId>
            <daysOfWeek>2;3;4;5;6</daysOfWeek>
          </org.lockss.daemon.CrawlWindows-Daily>
          <org.lockss.plugin.RateLimiterInfo>
            <rate>1/12h</rate>
          </org.lockss.plugin.RateLimiterInfo>
        </entry>
        <!-- pause crawls Sunday 2am through 10am Eastern (8h) -->
        <entry>
          <org.lockss.daemon.CrawlWindows-Daily>
            <from>2:00</from>
            <to>10:00</to>
            <timeZoneId>US/Eastern</timeZoneId>
            <daysOfWeek>1</daysOfWeek>
          </org.lockss.daemon.CrawlWindows-Daily>
          <org.lockss.plugin.RateLimiterInfo>
            <rate>1/8h</rate>
          </org.lockss.plugin.RateLimiterInfo>
        </entry>
        <!-- the rest of the time, 1/3s -->
        <entry>
          <org.lockss.daemon.CrawlWindows-Always />
          <org.lockss.plugin.RateLimiterInfo>
            <rate>1/3s</rate>
          </org.lockss.plugin.RateLimiterInfo>
        </entry>
      </cond>
    </org.lockss.plugin.RateLimiterInfo>
