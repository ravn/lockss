  <entry>
    <string>au_name</string>
    <string>"American Meteorological Society Plugin, Base URL %s, Journal ID %s, Volume %s", base_url, journal_id, volume_name</string>
  </entry>
  <entry>
    <string>au_start_url</string>
<!-- the TOC for a specific year (volume) is on a lockss page.The last bit can be index.html, nothing or any issue # seemingly -->
    <string>"%slockss/%s/%s/index.html", base_url, journal_id, volume_name</string>
  </entry>
  <entry>
    <string>au_feature_urls</string>
    <map>
      <entry>
        <string>au_title</string>
        <string>"%sloi/%s", base_url, journal_id</string>
      </entry>
      <entry>
        <string>au_volume</string>
        <string>"%slockss/%s/%s/1", base_url, journal_id, volume_name</string>
      </entry>
    </map>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
    <!--  Don't need to explicitly include the start URL in the crawl rules -->
      <string>1,"^https?://%s.*\.(bmp|css|gif|ico|jpe?g|js|png|tif?f)$", base_url_host</string>   
      <string>1,"^%stoc/%s/%s/[0-9]+", base_url, journal_id, volume_name</string>
      <string>2,"^%sdoi/(pdf|full|abs)/[0-9]+\.[0-9]+/[-.9]+$", base_url</string>
      <string>1,"^%sdoi/(pdf|full|abs|suppl)/[.0-9]+/[^/]+$", base_url</string>
      <!-- supplement files are stored even lowern under doi/suppl as zip, pdf, etc -->
      <string>1,"^%sdoi/suppl/[.0-9]+/[^/]+/suppl_file/", base_url</string>
      <!-- figures and tables images are stored under this -->
      <string>1,"^%sna101/home/literatum/publisher/ams/journals/content/%s.*\.(bmp|css|gif|ico|jpe?g|js|png|tif?f)$", base_url, journal_id</string>
    </list>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>journal_id</key>
        <displayName>Journal Identifier</displayName>
        <description>Identifier for journal (often used as part of file names)</description>
        <type>1</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>volume_name</key>
        <displayName>Volume</displayName>
        <type>1</type>
        <size>20</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>
  <entry>
    <string>au_substance_url_pattern</string>
    <list>
      	<string>"^%sdoi/(pdf|full|pdfplus)/[.0-9]+/[^/]+$", base_url</string>
    </list>
  </entry>
  <entry>
    <string>plugin_au_config_user_msg</string>
    <string>"Atypon Systems hosts this American Meteorological Society archival unit (AU) and require that you &lt;a href=&apos;%saction/institutionLockssIpChange&apos;&gt;register the IP address of this LOCKSS box in your institutional American Meteorological Society account&lt;/a&gt; before allowing your LOCKSS box to harvest the AU. Failure to comply with this publisher requirement may trigger crawler traps, and your LOCKSS box or your entire institution may be temporarily banned from accessing the site. You only need to register the IP address of your LOCKSS box once for all American Meteorological Society AUs.", base_url</string>
  </entry>
