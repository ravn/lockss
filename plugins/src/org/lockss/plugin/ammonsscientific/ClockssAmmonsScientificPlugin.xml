  <entry>
    <!-- example valid abs/pdf urls
    http://www.amsciepub.com/doi/pdf/10.2466/pms.110.2.353-354
    http://www.amsciepub.com/doi/abs/10.2466/09.02.10.CP.1.17
    http://www.amsciepub.com/doi/pdf/10.2466/03.11.IT.1.1
    http://www.amsciepub.com/doi/abs/10.2466/01.17.21.PR0.107.4.3-14
    http://www.amsciepub.com/toc/pr0/107/1
    http://www.amsciepub.com/toc/cp/1/1
    -->
    <string>au_crawlrules</string>
    <list>
      <string>1,"^https?://%s/.*\.(bmp|css|gif|ico|jpe?g|js|png|tif?f)(_v[0-9]+)?$", base_url_host</string>
      <string>4,"^%s.*%s", base_url, journal_abbr</string>
      <string>2,"^%spage",base_url</string>
      <string>1,"^%stoc/%s/%s/", base_url, journal_abbr, volume_name</string>
      <string>1,"^%sdoi/(pdf|pdfplus|abs|full|suppl)/\d+\.\d+/(\d+\.){0,5}%s\.%s.*", base_url, journal_abbr, volume_name</string>
    </list>
  </entry> 
  <entry>
    <string>au_substance_url_pattern</string>
