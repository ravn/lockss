{
  publisher <
    name = Faculdades Metropolitanas Unidas
  >

    plugin = org.lockss.plugin.ojs2.OJS2Plugin
    implicit < status ; year ; volume ; name ; param[year] ; comment[error] >
    comment[notReady] = No permission statement on manifest page.

  {

      title <
        name = Revista de Administração e Desenvolvimento Sustentável ;
        eissn = 2316-2651
      >

      comment[publisher] = Faculdades Metropolitanas Unidas
      param[base_url] = http://www.revistaseletronicas.fmu.br/
      param[journal_id] = remades

      au < exists ; 2011 ; 1 ; Revista de Administração e Desenvolvimento Sustentável Volume 1 ; 2011 ; >
      au < exists ; 2012 ; 2 ; Revista de Administração e Desenvolvimento Sustentável Volume 2 ; 2012 ; >
      au < expected ; 2013 ; 3 ; Revista de Administração e Desenvolvimento Sustentável Volume 3 ; 2013 ; >

  }

  {

      title <
        name = Revista FMU Direito ;
        eissn = 2316-1515
      >

      comment[publisher] = Faculdades Metropolitanas Unidas
      param[base_url] = http://www.revistaseletronicas.fmu.br/
      param[journal_id] = RMDIR

      au < exists ; 2009 ; 23 ; Revista FMU Direito Volume 23 ; 2009 ; >
      au < exists ; 2010 ; 24 ; Revista FMU Direito Volume 24 ; 2010 ; >
      au < exists ; 2011 ; 25 ; Revista FMU Direito Volume 25 ; 2011 ; >
      au < exists ; 2012 ; 26 ; Revista FMU Direito Volume 26 ; 2012 ; >
      au < exists ; 2013 ; 27 ; Revista FMU Direito Volume 27 ; 2013 ; >
      au < expected ; 2014 ; 28 ; Revista FMU Direito Volume 28 ; 2014 ; >

  }

}
