{

  publisher <
    name = American Society for Microbiology ;
    info[contract] = 2099 ;
    info[tester] = 0
  >

  {
    plugin = org.lockss.plugin.asm.ClockssASMscienceJournalPlugin
    param[base_url] = http://www.asmscience.org/
    implicit < status ; status2 ; year ; name ; param[volume_name] >
    comment[platform] = Pub2Web
  
    {
      title <
        name = EcoSal Plus ;
        eissn = 2324-6200
      >
      
      comment[structure] = this seems to be organized as a reference, not as a journal.
      comment[structure] = this has be reorganized by volume
      
      param[journal_id] = ecosalplus
      
      au < exists ; exists ; 2004-2005 ; EcoSal Plus Volume 1 ; 1 >
      au < exists ; exists ; 2006-2007 ; EcoSal Plus Volume 2 ; 2 >
      au < testing ; exists ; 2008-2009 ; EcoSal Plus Volume 3 ; 3 >
      au < testing ; exists ; 2010-2011 ; EcoSal Plus Volume 4 ; 4 >
      au < testing ; exists ; 2012-2013 ; EcoSal Plus Volume 5 ; 5 >
      au < exists ; exists ; 2014-2015 ; EcoSal Plus Volume 6 ; 6 >

    }
  
    {
      title <
        name = Microbiology Spectrum ;
        eissn = 2165-0497
      >
      
      param[journal_id] = microbiolspec
      
      au < testing ; exists ; 2013 ; Microbiology Spectrum 2013 ; 1 >
      au < testing ; exists ; 2014 ; Microbiology Spectrum 2014 ; 2 >
      au < testing ; exists ; 2015 ; Microbiology Spectrum 2015 ; 3 >

    }

  }

}
