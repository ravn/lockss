{

  publisher <
    name = Librello ;
    info[contract] = 2013 ;
    info[tester] = 4
  >

      plugin = org.lockss.plugin.ojs2.ClockssOJS2Plugin
      implicit < status ; status2 ; year ; name ; volume ; param[year] >
      
      param[base_url] = http://www.librelloph.com/

  {
    title <
      name = Challenges in Sustainability 
    >

      param[journal_id] = challengesinsustainability
