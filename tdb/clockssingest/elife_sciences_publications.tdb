{
  publisher <
    name = eLife Sciences Publications ;
    info[contract] = 2014 ;
    info[tester] = 4
  >

  plugin = org.lockss.plugin.highwire.elife.ClockssELifeDrupalPlugin
  implicit < status ; status2 ; year ; name ; param[volume_name] >
  
  {
    title <
      name = eLife ;
      issn = 2050-084X  
    >

    comment[platform] = HighWire Drupal
    param[base_url] = http://elifesciences.org/

      au < crawling ; exists ; 2012 ; eLife : Volume 2012 ; 2012 >
      au < crawling ; exists ; 2013 ; eLife : Volume 2013 ; 2013 >
      au < manifest ; exists ; 2014 ; eLife : Volume 2014 ; 2014 >
      au < expected ; exists ; 2015 ; eLife : Volume 2015 ; 2015 >

  }
}
