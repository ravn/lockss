{

  publisher <
    name = PeerJ ;
    info[tester] = 0
  >

  comment[platform] = Developed by Highwire.
  comment[volume] = Use year as volume name
  
  param[base_url] = https://peerj.com/
    
  implicit < status ; year ; name ; param[year] >

  {
  
    title < 
      name = PeerJ Archives
    >    
  
    plugin = org.lockss.plugin.peerj.PeerJPlugin
    param[peerj_site] = archives
    
    au < exists ; 2013 ; PeerJ Archives: 2013 ; 2013 > 
    
  }
  
  {
  
    title < 
      name = PeerJ PrePrints
    >

    plugin = org.lockss.plugin.peerj.ClockssPeerJPlugin
    param[peerj_site] = archives-preprints
    
    au < exists ; 2013 ; PeerJ PrePrint Archives: 2013 ; 2013 > 

  }

}
