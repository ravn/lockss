    implicit < status ; year ; name ; param[year] >

  {
  
    title < 
      name = PeerJ Archives ;
      issn = 2167-8359
    >    
  
    param[peerj_site] = archives
    
    au < exists ; 2013 ; PeerJ Archives: 2013 ; 2013 > 
    
  }
  
  {
  
    title < 
      name = PeerJ PrePrints ;
      issn = 2167-9843
    >

    param[peerj_site] = archives-preprints
    
    au < exists ; 2013 ; PeerJ PrePrint Archives: 2013 ; 2013 > 

  }

}
