{
  publisher <
    name = Spandidos Publications
  >

  plugin = needs.plugin
  param[base_url] = http://www.spandidos-publications.com/
  implicit < status ; year ; name ; param[volume_name] >

  {
    title <
      name = Oncology Letters ;
      issn = 1792-1074 ;
      eissn = 1792-1082
    >

    param[journal_id] = ol

    au < exists ; 2010 ; Oncology Letters Volume 1 ; 1 >
    au < exists ; 2011 ; Oncology Letters Volume 2 ; 2 >
    au < exists ; 2012 ; Oncology Letters Volume 3 ; 3 >
    au < exists ; 2012 ; Oncology Letters Volume 4 ; 4 >
    au < exists ; 2013 ; Oncology Letters Volume 5 ; 5 >
    au < exists ; 2013 ; Oncology Letters Volume 6 ; 6 >

  }

  {
    title <
      name = International Journal of Oncology ;
      issn = 1792-1074 ;
      eissn = 1792-1082
    >

    param[journal_id] = ijo

    au < exists ; 2010 ; International Journal of Oncology Volume 1 ; 1 >
    au < exists ; 2011 ; International Journal of Oncology Volume 2 ; 2 >
    au < exists ; 2012 ; International Journal of Oncology Volume 3 ; 3 >
    au < exists ; 2012 ; International Journal of Oncology Volume 4 ; 4 >
    au < exists ; 2013 ; International Journal of Oncology Volume 5 ; 5 >
    au < exists ; 2013 ; International Journal of Oncology Volume 6 ; 6 >

  }

}
