{
  publisher <
    name = eLife Sciences Publications ;
    info[tester] = 2
  >

  plugin = org.lockss.plugin.highwire.elife.ELifeDrupalPlugin
  implicit < status ; year ; name ; param[volume_name] >
  
  {
    title <
      name = eLife ;
      issn = 2050-084X  
    >

    comment[platform] = HighWire Drupal
    param[base_url] = http://elifesciences.org/

    comment[0] = moving to new platform fall 2015

    {
      au < doNotProcess ; 2012 ; eLife : Year 2012 ; 2012 >
      au < doNotProcess ; 2013 ; eLife : Year 2013 ; 2013 >
      au < ready ; 2014 ; eLife : Year 2014 ; 2014 >
    }
  }
}
