    plugin = org.lockss.plugin.projmuse.ProjectMusePlugin
    param[base_url] = http://muse.jhu.edu/
    param[journal_dir] = italian_culture
    implicit < status ; year ; name ; param[volume] ; volume >
    attr[journal_id] = itc

    au < released ; 2005 ; Italian Culture Volume 23 ; 23 ; 23 >
    au < released ; 2006-2007 ; Italian Culture Volume 24-25 ; 24 ; 24-25 >
    au < released ; 2008 ; Italian Culture Volume 26 ; 26 ; 26 >

  }

}